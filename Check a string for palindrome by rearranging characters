#include <bits/stdc++.h>
using namespace std;

int main() {
    int odd=0,ct=0,i,c[26]={0};
	    memset(c,0,sizeof(c));
	    string a;
	    cin>>a;
	    for(i=0;i<a.length();i++)
	    {
	        c[a[i]-97]++;                  //count array
	    }
	    for(i=0;i<26;i++)
	    {
	        if(c[i]%2==1)
	        odd++;                        //counts characters with  odd occurrences
	    }
	    if(odd<=1)
	cout<<"Yes"<<endl;
	else
	cout<<"No"<<endl;
	}
	

